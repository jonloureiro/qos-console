{
  "private": true,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run build:css && remix build",
    "build:css": "tailwindcss -i ./app/tailwind.css -o ./app/style.css --minify",
    "dev": "cross-env NODE_ENV=development netlify dev",
    "dev:css": "tailwindcss -i ./app/tailwind.css -o ./app/style.css --watch",
    "postinstall": "remix setup node"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.50.0",
    "@netlify/functions": "^0.10.0",
    "@remix-run/netlify": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "cross-env": "^7.0.3",
    "daisyui": "^1.25.4",
    "tailwindcss": "^3.0.21",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}